{
    "version": 2,
    "builds": [

      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      },
      
      {
        "src": "server.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      
      {
        "src": "/api/(.*)",
        "dest": "server.mjs"
      }
    ]
  }